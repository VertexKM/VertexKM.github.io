            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- definition of variables element -->
  <xs:element name="Variables">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Variable" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
