  <!-- definition of objects element -->
  <xs:element name="Objects">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Book" minOccurs="0" />
        <xs:element ref="File" minOccurs="0" />
        <xs:element ref="Glossary" minOccurs="0" />
