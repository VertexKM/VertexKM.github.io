              <xs:element name="Locale" type="localetype" />
              <xs:element name="VariableAssignments" type="variableassignmentstype" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PublishJob" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="BookID" type="xs:nonNegativeInteger" />
