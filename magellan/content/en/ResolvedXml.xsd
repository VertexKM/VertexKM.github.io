        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- definition of ReleaseStates element -->
  <xs:element name="ReleaseStates">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ReleaseState" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ID" type="xs:nonNegativeInteger" />
