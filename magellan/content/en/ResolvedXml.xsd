              <xs:element name="VariableAssignments" type="variableassignmentstype" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="TranslationJob" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Description" type="string255" />
              <xs:element name="GUID" type="guid" />
              <xs:element name="WordCount" type="integer" />
              <xs:element name="ReturnDate" type="xs:dateTime" minOccurs="0" />
