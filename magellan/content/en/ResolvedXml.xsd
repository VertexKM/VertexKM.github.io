        <xs:element ref="TitlePage" minOccurs="0" />
        <xs:element ref="Topic" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- definition of object summarys element -->
  <xs:element name="ObjectSummarys">
