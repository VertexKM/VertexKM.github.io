  <!-- definition of authorit element -->
  <xs:element name="AuthorIT">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Objects" />
        <xs:element ref="ObjectSummarys" minOccurs="0" />
        <xs:element ref="Folders" minOccurs="0" />
        <xs:element ref="ReleaseStates" minOccurs="0" />
        <xs:element ref="Variables" minOccurs="0" />
        <xs:element ref="Information" minOccurs="0" />
        <xs:element ref="Reviews" minOccurs="0" />
      </xs:sequence>
      <xs:attribute name="version" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <!-- definition of folders element -->
  <xs:element name="Folders">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Folder" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ID" type="xs:nonNegativeInteger" />
              <xs:element name="Name" type="string255" />
              <xs:element name="GUID" type="guid" />
              <xs:element name="ParentFolderID" type="xs:nonNegativeInteger" />
              <xs:element name="HasSubFolders" type="xs:boolean" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- definition of information element -->
  <xs:element name="Information">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Description" type="string255" minOccurs="0" />
        <xs:element name="Date" type="xs:dateTime" minOccurs="0" />
        <xs:element name="Library" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Name" type="string255" />
              <xs:element name="GUID" type="guid" />
