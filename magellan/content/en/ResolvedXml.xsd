            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- definition of Reviews element -->
  <xs:element name="Reviews">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Review" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
